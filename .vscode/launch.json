{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "to-dxf",
      "type": "debugpy",
      "request": "launch",
      "module": "mapsys.__main__",
      "args": [
        "--trace",
        "--log-file",
        "${workspaceFolder}/playground/mapsys-to-dxf.log",
        "to-dxf",
        // "${workspaceFolder}/playground/sample",
        // "D:/prog/only4me/mapsys/playground/fisiere/Simplu/Adi",
        "E:/Advaita/altele/Resurse pe UAT-uri/Voslobeni/vice/tarlale(numai)",
        "--dxf",
        "${workspaceFolder}/playground/mapsys-output.dxf",
        "--dxf-template",
        "${workspaceFolder}/playground/template.dxf",
        "--point-block",
        "PCT3D",
        "--point-name-attrib",
        "PTNAME",
        "--point-source-attrib",
        "SOURCE",
        "--point-z-attrib",
        "Z"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true,
      "windows": {
        "python": "${workspaceFolder}//venv//Scripts//python.exe"
      }
    },
    {
      "name": "to-xlsx",
      "type": "debugpy",
      "request": "launch",
      "module": "mapsys.__main__",
      "args": [
        "--trace",
        "--log-file",
        "${workspaceFolder}/playground/mapsys-to-xlsx.log",
        "to-xlsx",
        // Project directory containing a single .pr5
        "E:/Advaita/altele/Resurse pe UAT-uri/Voslobeni/vice/tarlale(numai)",
        // "${workspaceFolder}/playground/sample",
        "--xlsx",
        "${workspaceFolder}/playground/mapsys-output.xlsx"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true,
      "windows": {
        "python": "${workspaceFolder}//venv//Scripts//python.exe"
      }
    },
    {
      "name": "to-xlsx-dir",
      "type": "debugpy",
      "request": "launch",
      "module": "mapsys.__main__",
      "args": [
        "--trace",
        "--log-file",
        "${workspaceFolder}/playground/mapsys-to-xlsx.log",
        "to-xlsx-dir",
        // Root directory to scan for .pr5 files
        "${workspaceFolder}/playground",
        "--max-depth",
        "1",
        "--exclude-backup"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true,
      "windows": {
        "python": "${workspaceFolder}//venv//Scripts//python.exe"
      }
    },
  ]
}
