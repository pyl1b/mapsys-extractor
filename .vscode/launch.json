{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "to-dxf",
      "type": "debugpy",
      "request": "launch",
      "module": "mapsys.__main__",
      "args": [
        "--trace",
        "--log-file",
        "${workspaceFolder}/playground/mapsys-to-dxf.log",
        "to-dxf",
        "${workspaceFolder}/playground/sample",
        "--dxf",
        "${workspaceFolder}/playground/mapsys-output.dxf",
        "--dxf-template",
        "${workspaceFolder}/playground/template.dxf",
        "--point-block",
        "PCT3D",
        "--point-name-attrib",
        "PTNAME",
        "--point-source-attrib",
        "SOURCE",
        "--point-z-attrib",
        "Z"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": true,
      "windows": {
        "python": "${workspaceFolder}//venv//Scripts//python.exe"
      }
    },
  ]
}
